@model jbH60Store.Models.ProductCategory

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="edit-form-container">
    <h1>Edit</h1>
    <h4>Product Category</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-10">
            @Html.DisplayNameFor(model => model.ProdCat)
        </dt>
        <dd class="col-sm-4">
            @Html.DisplayFor(model => model.ProdCat)
        </dd>
    </dl><div>
        <a asp-action="Edit" asp-route-id="@Model?.CategoryId" class="btn btn-edit">
            <img src="~/images/edit.png" /> Edit
        </a> |
        <a asp-action="AllProductCategoriesSortedByCategory">
            Back to List
        </a>
    </div>
</div>


@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}